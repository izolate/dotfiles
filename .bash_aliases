# Aliases
alias ls='gls -F --color=auto'
alias ll='gls -Flashp --color=auto'
alias cp='cp -iv'
alias mv='mv -iv'
alias mkdir='mkdir -pv'
alias ..='cd ../'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'
alias vi=$EDITOR
alias edit=$EDITOR
alias pyclean='find . -name '*.pyc' -delete'
alias v='f -e vim' # quick opening files with vim
